<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="viewModel"
            type="com.penoder.dailynews.ui.splash.RegisterActivity" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.penoder.mylibrary.view.CustomTitle
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:leftIcon="@drawable/icon_back"
            app:rightTitle="@string/login"
            app:rightTitleClickCommand="@{viewModel.onLoginCommand}"
            app:titleTxt="@string/register" />

    </LinearLayout>

</layout>